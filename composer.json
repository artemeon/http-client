{
  "name": "artemeon/http-client",
  "license": "proprietary",
  "type": "library",
  "description": "Thin wrapper for external http client libraries.",
  "keywords": [
    "php7",
    "http",
    "client"
  ],
  "authors": [
    {
      "name": "Dietmar Simons",
      "email": "dietmar.simons@artemeon.de"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Artemeon\\HttpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Artemeon\\HttpClient\\Tests\\": "tests/Unit/"
    }
  },
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.*",
    "squizlabs/php_codesniffer": "3.*",
    "php-mock/php-mock-prophecy": "0.0.2",
    "mikey179/vfsStream": "1.6.*"
  },
  "minimum-stability": "stable"
}
