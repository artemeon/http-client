{
  "name": "artemeon/http-client",
  "license": "MIT",
  "type": "library",
  "description": "Thin wrapper for external http client libraries.",
  "keywords": [
    "php7",
    "http",
    "client"
  ],
  "authors": [
    {
      "name": "Dietmar Simons",
      "email": "dietmar.simons@artemeon.de"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Artemeon\\HttpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Artemeon\\HttpClient\\Tests\\": "tests/unit/"
    }
  },
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "3.*",
    "php-mock/php-mock-prophecy": "0.0.2",
    "mikey179/vfsstream": "1.6.*",
    "php-http/psr7-integration-tests": "@dev"
  },
  "minimum-stability": "stable"
}
